# /etc/profile.d/intel-proxy.csh - set intel-proxy stuff
setenv HTTP_PROXY=http://proxy-us.intel.com:911
setenv HTTPS_PROXY=http://proxy-us.intel.com:911
setenv FTP_PROXY=proxy-us.intel.com:911
setenv SOCKS_PROXY=proxy-skype.intel.com:1080
setenv NO_PROXY=127.0.0.1,localhost,<%= hostname %>,<%= fqdn %>,.intel.com,<%= ipaddress %>,10.0.0.0/8,192.168.0.0/16
setenv http_proxy=http://proxy-us.intel.com:911
setenv https_proxy=http://proxy-us.intel.com:911
setenv ftp_proxy=proxy-us.intel.com:911
setenv socks_proxy=proxy-skype.intel.com:1080
setenv no_proxy=127.0.0.1,localhost,<%= hostname %>,<%= fqdn %>,.intel.com,<%= ipaddress %>,10.0.0.0/8,192.168.0.0/16
